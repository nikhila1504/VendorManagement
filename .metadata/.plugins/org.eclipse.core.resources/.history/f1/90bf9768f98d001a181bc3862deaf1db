eureka:         #tells about the Eureka server details and its refresh time
  instance:
    leaseRenewalIntervalInSeconds: 21
    leaseExpirationDurationInSeconds: 25
  client:
    serviceUrl:
      defaultZone: http://127.0.0.1:8761/eureka/
    healthcheck:
      enabled: true
    lease:
      duration: 25
 
spring:     
  application:
    name: ProductManagement   #current service name to be used by the eureka server
    
